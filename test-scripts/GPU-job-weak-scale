#!/bin/bash
#SBATCH -C gpu
#SBATCH -G 1
#SBATCH --job-name=gpu_cpu_ws_weak_small
#SBATCH --output=gpu_cpu_ws_weak_small.out
#SBATCH --error=gpu_cpu_ws_weak_small.err
#SBATCH -t 10
#SBATCH -c 10

srun ./program_gpu dna ../test-data/dna-reference.fasta ../test-data/dna-query.fasta ./out_file 1 7 1
